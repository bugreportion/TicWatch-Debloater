<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <SolidColorBrush x:Key="AccentColor" Color="PLACEHOLDER" />
  <SolidColorBrush x:Key="TextColor" Color="#ffffff" />
  <SolidColorBrush x:Key="DisabledTextColor" Color="#6e6e6e" />
  <SolidColorBrush x:Key="BackgroundColor" Color="#202020" />
  <SolidColorBrush x:Key="ButtonBackgroundColor" Color="#ffffff" Opacity="0.065" />
  <SolidColorBrush x:Key="DisabledButtonBackgroundColor" Color="#ffffff" Opacity="0.04" />
  <SolidColorBrush x:Key="ActiveButtonBackgroundColor" Color="#ffffff" Opacity="0.085" />
  <SolidColorBrush x:Key="ScrollBarColor" Color="#a0a0a0" />
</ResourceDictionary>
