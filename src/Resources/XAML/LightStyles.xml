<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <SolidColorBrush x:Key="AccentColor" Color="PLACEHOLDER" />
  <SolidColorBrush x:Key="TextColor" Color="#000000" />
  <SolidColorBrush x:Key="DisabledTextColor" Color="#9d9d9d" />
  <SolidColorBrush x:Key="BackgroundColor" Color="#f3f3f3" />
  <SolidColorBrush x:Key="ButtonBackgroundColor" Color="#ffffff" Opacity="0.5" />
  <SolidColorBrush x:Key="DisabledButtonBackgroundColor" Color="#ffffff" Opacity="0.3" />
  <SolidColorBrush x:Key="ActiveButtonBackgroundColor" Color="#000000" Opacity="0.02" />
  <SolidColorBrush x:Key="ScrollBarColor" Color="#a0a0a0" />
</ResourceDictionary>
